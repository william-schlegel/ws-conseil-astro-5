    from {
      fill: #00467c;
    }
    to {
      fill: #2cdd9b;
    }
  }

  @keyframes inno-verre {
    from {
      fill: #00213a;
    }
    to {
      fill: white;
    }
  }

  @keyframes inno-man-head {
    from {
      transform: rotateZ(-5deg);
    }
    to {
      transform: rotateZ(10deg);
    }
  }
</style>
