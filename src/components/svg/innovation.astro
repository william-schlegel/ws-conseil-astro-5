  }
  #light {
    animation: fade-in 3s alternate infinite ease-in-out;
  }
  #man-head {
    animation: inno-man-head 4s alternate infinite ease-in-out;
    transform-origin: center;
    transform-box: fill-box;
  }

  @keyframes inno-bulb {
    from {
      transform: scale(0.95);
    }
    to {
      transform: scale(1.05);
    }
  }

